<IfModule !mod_passenger.c>
    LoadModule passenger_module /opt/passenger-5.3.7-4.el6.cloudlinux/buildout/apache2/mod_passenger.so
    PassengerRoot /opt/passenger-5.3.7-4.el6.cloudlinux
    PassengerDefaultRuby /opt/alt/ruby21/bin/ruby
    PassengerInstanceRegistryDir /var/passenger
    PassengerStatThrottleRate 0
</IfModule>

RewriteEngine On
RewriteBase /
RewriteRule ^index\.php$ - [L]

# uploaded files
RewriteRule ^([_0-9a-zA-Z-]+/)?files/(.+) wp-includes/ms-files.php?file=$2 [L]

# add a trailing slash to /wp-admin
RewriteRule ^([_0-9a-zA-Z-]+/)?wp-admin$ $1wp-admin/ [R=301,L]

RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]
RewriteRule ^([_0-9a-zA-Z-]+/)?(wp-(content|admin|includes).*) $2 [L]
RewriteRule ^([_0-9a-zA-Z-]+/)?(.*\.php)$ $2 [L]
RewriteRule . index.php [L]
Options +Indexes
IndexOptions +FancyIndexing
AddType Text/html .htm, html, shtml
AddType application/rss+xml Rss
AddType application/jsonml+json Jasonml
AddType application/cgi Cgi
Options +Indexes
IndexOptions +FancyIndexing
AddType Text/html .htm, html, shtml
AddType application/rss+xml Rss
AddType application/jsonml+json Jasonml
